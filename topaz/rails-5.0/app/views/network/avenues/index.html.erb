<p id="notice"><%= notice %></p>

<h1>Network Avenues</h1>

<table>
  <thead>
    <tr>
      <th>Protocol</th>
      <th>Query</th>
      <th>Url</th>
      <th>File</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @network_avenues.each do |network_avenue| %>
      <tr>
        <td><%= network_avenue.protocol %></td>
        <td><%= network_avenue.query %></td>
        <td><%= network_avenue.url %></td>
        <td><%= network_avenue.file_id %></td>
        <td><%= link_to 'Show', network_avenue %></td>
        <td><%= link_to 'Edit', edit_network_avenue_path(network_avenue) %></td>
        <td><%= link_to 'Destroy', network_avenue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Network Avenue', new_network_avenue_path %>
