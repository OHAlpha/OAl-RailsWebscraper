<p id="notice"><%= notice %></p>

<h1>Network Header Sets</h1>

<table>
  <thead>
    <tr>
      <th>Headers string</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @network_header_sets.each do |network_header_set| %>
      <tr>
        <td><%= network_header_set.headers_string %></td>
        <td><%= link_to 'Show', network_header_set %></td>
        <td><%= link_to 'Edit', edit_network_header_set_path(network_header_set) %></td>
        <td><%= link_to 'Destroy', network_header_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Network Header Set', new_network_header_set_path %>
